{
  "name": "@stillrivercode/agentic-workflow-template",
  "version": "1.11.3",
  "description": "NPM package to create AI-powered GitHub workflow automation projects",
  "keywords": [
    "github-actions",
    "ai",
    "automation",
    "workflow",
    "template",
    "cli"
  ],
  "author": "Still River Code <noreply@stillrivercode.com>",
  "license": "MIT",
  "homepage": "https://github.com/stillrivercode/agentic-workflow-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stillrivercode/agentic-workflow-template.git"
  },
  "bugs": {
    "url": "https://github.com/stillrivercode/agentic-workflow-template/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "scripts/",
    ".github/workflows/",
    "docs/",
    "README.md",
    "CLAUDE.md",
    "GEMINI.md",
    "LICENSE",
    "install.sh",
    "shared-commands/commands/",
    "shared-commands/lib/",
    "shared-commands/setup.sh",
    "shared-commands/index.js",
    "shared-commands/postinstall.js",
    "shared-commands/README.md",
    "package.json",
    ".pre-commit-config.yaml",
    "eslint.config.js",
    ".yamllint.yaml",
    ".secrets.baseline",
    ".gitignore",
    ".gitattributes",
    ".env.example"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.0",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-security": "^3.0.1",
    "@eslint/js": "^9.0.0",
    "jest": "^29.0.0",
    "markdownlint-cli": "^0.37.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "yaml-lint": "^1.7.0"
  },
  "scripts": {
    "test": "cd app && npm test",
    "test:config": "node -e \"console.log('Running config validation tests...'); try { require('./eslint.config.js'); console.log('✅ ESLint config valid'); } catch(e) { console.error('❌ ESLint config error:', e.message); process.exit(1); }\"",
    "lint": "eslint shared-commands/",
    "lint:fix": "eslint shared-commands/ --fix",
    "lint:cached": "eslint --cache shared-commands/",
    "lint:security": "eslint shared-commands/",
    "lint:yaml": "npx yaml-lint .github/workflows/*.yml .github/workflows/*.yaml *.yml *.yaml",
    "lint:markdown": "npx markdownlint *.md docs/*.md",
    "lint:all": "npm-run-all --parallel lint:cached lint:yaml lint:markdown format:check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "echo 'Use: npx @stillrivercode/agentic-workflow-template <project-name>' && echo 'Example: npx @stillrivercode/agentic-workflow-template my-ai-project'",
    "create-project": "node cli/index.js",
    "test:cli": "./dev-scripts/test-cli.sh",
    "test:cli-integration": "jest cli/tests/validators.test.js",
    "test:all": "npm run test:cli-integration && npm run test:cli"
  },
  "main": "cli/index.js",
  "preferGlobal": true
}
